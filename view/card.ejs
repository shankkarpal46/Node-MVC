<%- include('link') %>

<% users.forEach(user => { %>
<div class="card" style="width: 18rem;">
    <img src="..." class="card-img-top" alt="...">
    <div class="card-body">
        <h5 class="card-title">
            <%= user.firstName%> <%= user.lastName %>
        </h5>
        <p class="card-text"><%= user.email %></p>
        <a href="#" class="btn btn-primary">Go somewhere</a>
    </div>
</div>
<% }) %>

